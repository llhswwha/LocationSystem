<Window x:Class="LocationServer.Windows.Simple.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationServer.Windows.Simple"
        mc:Ignorable="d"
        Title="InputWindow" Height="100" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="Input"></TextBox>
        <Button Grid.Row="1" Name="BtnOk" Click="BtnOk_Click">确定</Button>
    </Grid>
</Window>
